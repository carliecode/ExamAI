    "def left_rotate_string(input_text) -> str:\n",
    "    try:\n",
    "        left = input_text[:3]\n",
    "        right = input_text[3:]\n",
    "        return right + left\n",
    "    except Exception as e:\n",
    "        raise e\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(left_rotate_string('hello'))"
   ]
  }
